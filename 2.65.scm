         (cons (car set1)
               (intersection-set (cdr set1) set2)))
        (else (intersection-set (cdr set1) set2))))

(define (intersection-set set1 set2)
  (cond ((or (null? set1) (null? set2)) â€™())
        ((element-of-set? (car set1) set2)
         (cons (car set1)
